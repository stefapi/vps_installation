     <text:p text:style-name="P435"><text:span text:style-name="ExtensionTok">/usr/bin/borg</text:span><text:span text:style-name="NormalTok"> info </text:span><text:span text:style-name="AttributeTok">--progress</text:span><text:span text:style-name="NormalTok"> borgbackup@</text:span><text:span text:style-name="OperatorTok">&lt;</text:span><text:span text:style-name="NormalTok">storing_srv</text:span><text:span text:style-name="OperatorTok">&gt;</text:span><text:span text:style-name="NormalTok">:/home/borgbackup/borgbackup/::</text:span><text:span text:style-name="VariableTok">$1</text:span> </text:p>
