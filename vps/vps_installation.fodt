     <text:p text:style-name="P230"><text:span text:style-name="NormalTok"><text:s text:c="16"/>&lt;</text:span><text:span text:style-name="KeywordTok">Server</text:span><text:span text:style-name="NormalTok">&gt;mail.example.com&lt;/</text:span><text:span text:style-name="KeywordTok">Server</text:span><text:span text:style-name="NormalTok">&gt; </text:span></text:p>
     <text:p text:style-name="P230"><text:span text:style-name="NormalTok"><text:s text:c="16"/>&lt;</text:span><text:span text:style-name="KeywordTok">Port</text:span><text:span text:style-name="NormalTok">&gt;465&lt;/</text:span><text:span text:style-name="KeywordTok">Port</text:span><text:span text:style-name="NormalTok">&gt;</text:span> </text:p>
     <text:p text:style-name="P230"><text:span text:style-name="NormalTok"><text:s text:c="16"/>&lt;</text:span><text:span text:style-name="KeywordTok">DomainRequired</text:span><text:span text:style-name="NormalTok">&gt;on&lt;/</text:span><text:span text:style-name="KeywordTok">DomainRequired</text:span><text:span text:style-name="NormalTok">&gt;</text:span> </text:p>
