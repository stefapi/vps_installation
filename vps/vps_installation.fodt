     <text:p text:style-name="P230"><text:span text:style-name="NormalTok"><text:s text:c="12"/>&lt;</text:span><text:span text:style-name="KeywordTok">Protocol</text:span><text:span text:style-name="NormalTok">&gt;</text:span> </text:p>
