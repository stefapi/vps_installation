     <text:p text:style-name="P467"><text:span text:style-name="ExtensionTok">docker</text:span><text:span text:style-name="NormalTok"> run </text:span><text:span text:style-name="AttributeTok">-d</text:span><text:span text:style-name="NormalTok"> </text:span><text:span text:style-name="DataTypeTok">\</text:span> </text:p>
     <text:p text:style-name="P467"><text:span text:style-name="NormalTok"><text:s text:c="2"/></text:span><text:span text:style-name="AttributeTok">--name</text:span><text:span text:style-name="OperatorTok">=</text:span><text:span text:style-name="NormalTok">wg-easy </text:span><text:span text:style-name="DataTypeTok">\</text:span> </text:p>
     <text:p text:style-name="P467"><text:span text:style-name="NormalTok"><text:s text:c="2"/></text:span><text:span text:style-name="AttributeTok">-e</text:span><text:span text:style-name="NormalTok"> LANG=fr </text:span><text:span text:style-name="DataTypeTok">\</text:span> </text:p>
     <text:p text:style-name="P467"><text:span text:style-name="NormalTok"><text:s text:c="2"/></text:span><text:span text:style-name="AttributeTok">-e</text:span><text:span text:style-name="NormalTok"> WG_HOST=Public_IP </text:span><text:span text:style-name="DataTypeTok">\ </text:span></text:p>
     <text:p text:style-name="P467"><text:span text:style-name="NormalTok"><text:s text:c="2"/></text:span><text:span text:style-name="ExtensionTok">-e</text:span><text:span text:style-name="NormalTok"> PASSWORD=mot_de_passe </text:span><text:span text:style-name="DataTypeTok">\ </text:span></text:p>
     <text:p text:style-name="P467"><text:span text:style-name="NormalTok"><text:s text:c="2"/></text:span><text:span text:style-name="ExtensionTok">-e</text:span><text:span text:style-name="NormalTok"> PORT=51821 </text:span><text:span text:style-name="DataTypeTok">\</text:span> </text:p>
     <text:p text:style-name="P467"><text:span text:style-name="NormalTok"><text:s text:c="2"/></text:span><text:span text:style-name="AttributeTok">-e</text:span><text:span text:style-name="NormalTok"> WG_PORT=51820 </text:span><text:span text:style-name="DataTypeTok">\</text:span> </text:p>
     <text:p text:style-name="P467"><text:span text:style-name="NormalTok"><text:s text:c="2"/></text:span><text:span text:style-name="AttributeTok">-v</text:span><text:span text:style-name="NormalTok"> /etc/wg-easy:/etc/wireguard </text:span><text:span text:style-name="DataTypeTok">\</text:span> </text:p>
     <text:p text:style-name="P467"><text:span text:style-name="NormalTok"><text:s text:c="2"/></text:span><text:span text:style-name="AttributeTok">-p</text:span><text:span text:style-name="NormalTok"> 51820:51820/udp </text:span><text:span text:style-name="DataTypeTok">\</text:span> </text:p>
     <text:p text:style-name="P467"><text:span text:style-name="NormalTok"><text:s text:c="2"/></text:span><text:span text:style-name="AttributeTok">-p</text:span><text:span text:style-name="NormalTok"> 51821:51821/tcp </text:span><text:span text:style-name="DataTypeTok">\</text:span> </text:p>
     <text:p text:style-name="P467"><text:span text:style-name="NormalTok"><text:s text:c="2"/></text:span><text:span text:style-name="AttributeTok">--cap-add</text:span><text:span text:style-name="OperatorTok">=</text:span><text:span text:style-name="NormalTok">NET_ADMIN </text:span><text:span text:style-name="DataTypeTok">\</text:span> </text:p>
     <text:p text:style-name="P467"><text:span text:style-name="NormalTok"><text:s text:c="2"/></text:span><text:span text:style-name="AttributeTok">--cap-add</text:span><text:span text:style-name="OperatorTok">=</text:span><text:span text:style-name="NormalTok">SYS_MODULE </text:span><text:span text:style-name="DataTypeTok">\</text:span> </text:p>
     <text:p text:style-name="P467"><text:span text:style-name="NormalTok"><text:s text:c="2"/></text:span><text:span text:style-name="AttributeTok">--sysctl</text:span><text:span text:style-name="OperatorTok">=</text:span><text:span text:style-name="StringTok">&quot;net.ipv4.conf.all.src_valid_mark=1&quot;</text:span><text:span text:style-name="NormalTok"> </text:span><text:span text:style-name="DataTypeTok">\</text:span> </text:p>
