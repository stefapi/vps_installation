     <text:p text:style-name="P110"><text:span text:style-name="NormalTok"><text:s text:c="4"/></text:span><text:span text:style-name="ControlFlowTok">for</text:span><text:span text:style-name="NormalTok"> (</text:span><text:span text:style-name="BuiltInTok">file</text:span><text:span text:style-name="NormalTok">, index) </text:span><text:span text:style-name="KeywordTok">in</text:span><text:span text:style-name="NormalTok"> pkg.file_list:</text:span> </text:p>
