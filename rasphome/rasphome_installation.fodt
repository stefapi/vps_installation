     <text:p text:style-name="P110"><text:span text:style-name="ControlFlowTok">except</text:span><text:span text:style-name="NormalTok"> </text:span><text:span text:style-name="PreprocessorTok">SystemError</text:span><text:span text:style-name="NormalTok"> </text:span><text:span text:style-name="ImportTok">as</text:span><text:span text:style-name="NormalTok"> e:</text:span> </text:p>
     <text:p text:style-name="P110"><text:span text:style-name="NormalTok"><text:s text:c="4"/>sys.stderr.write(</text:span><text:span text:style-name="StringTok">&quot;Error: Couldn&apos;t mark the upgrade (</text:span><text:span text:style-name="SpecialCharTok">%s</text:span><text:span text:style-name="StringTok">)&quot;</text:span><text:span text:style-name="NormalTok"> </text:span><text:span text:style-name="OperatorTok">%</text:span><text:span text:style-name="NormalTok"> e)</text:span> </text:p>
