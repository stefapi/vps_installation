     <text:p text:style-name="P110"><text:span text:style-name="BuiltInTok">print</text:span><text:span text:style-name="NormalTok">(</text:span><text:span text:style-name="StringTok">&quot;</text:span><text:span text:style-name="SpecialCharTok">%d</text:span><text:span text:style-name="StringTok"> are security updates.&quot;</text:span><text:span text:style-name="NormalTok"> </text:span><text:span text:style-name="OperatorTok">%</text:span><text:span text:style-name="NormalTok"> security_upgrades)</text:span> </text:p>
     <text:p text:style-name="P110"><text:span text:style-name="BuiltInTok">print</text:span><text:span text:style-name="NormalTok">(</text:span><text:span text:style-name="StringTok">&quot;&quot;</text:span><text:span text:style-name="NormalTok">) <text:s/></text:span><text:span text:style-name="CommentTok"># leave a trailing blank line</text:span> </text:p>
    </text:list-item>
