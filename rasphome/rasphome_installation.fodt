     <text:p text:style-name="P110"><text:span text:style-name="NormalTok"><text:s text:c="4"/>current </text:span><text:span text:style-name="OperatorTok">=</text:span><text:span text:style-name="NormalTok"> pkg.current_ver</text:span> </text:p>
     <text:p text:style-name="P110"/>
     <text:p text:style-name="P110"><text:span text:style-name="NormalTok"><text:s text:c="4"/></text:span><text:span text:style-name="CommentTok"># skip packages not marked as upgraded/installed</text:span> </text:p>
     <text:p text:style-name="P110"><text:span text:style-name="NormalTok"><text:s text:c="4"/></text:span><text:span text:style-name="ControlFlowTok">if</text:span><text:span text:style-name="NormalTok"> </text:span><text:span text:style-name="KeywordTok">not</text:span><text:span text:style-name="NormalTok"> (depcache.marked_install(pkg) </text:span><text:span text:style-name="KeywordTok">or</text:span><text:span text:style-name="NormalTok"> depcache.marked_upgrade(pkg)):</text:span> </text:p>
