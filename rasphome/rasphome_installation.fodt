     <text:p text:style-name="P110"><text:span text:style-name="CommentTok"># setup a DepCache instance to interact with the repo</text:span> </text:p>
     <text:p text:style-name="P110"><text:span text:style-name="NormalTok">depcache </text:span><text:span text:style-name="OperatorTok">=</text:span><text:span text:style-name="NormalTok"> apt_pkg.DepCache(cache)</text:span> </text:p>
