     <text:p text:style-name="P110"><text:span text:style-name="ControlFlowTok">if</text:span><text:span text:style-name="NormalTok"> depcache.broken_count </text:span><text:span text:style-name="OperatorTok">&gt;</text:span><text:span text:style-name="NormalTok"> </text:span><text:span text:style-name="DecValTok">0</text:span><text:span text:style-name="NormalTok">:</text:span> </text:p>
