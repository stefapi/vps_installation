     <text:p text:style-name="P110"><text:span text:style-name="NormalTok"><text:s text:c="4"/>security_package_sources </text:span><text:span text:style-name="OperatorTok">=</text:span><text:span text:style-name="NormalTok"> [(</text:span><text:span text:style-name="StringTok">&quot;Ubuntu&quot;</text:span><text:span text:style-name="NormalTok">, </text:span><text:span text:style-name="StringTok">&quot;</text:span><text:span text:style-name="SpecialCharTok">%s</text:span><text:span text:style-name="StringTok">-security&quot;</text:span><text:span text:style-name="NormalTok"> </text:span><text:span text:style-name="OperatorTok">%</text:span><text:span text:style-name="NormalTok"> DISTRO),</text:span> </text:p>
